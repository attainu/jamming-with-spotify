{
  "author": "Spotify",
  "name": "web-api-auth-examples",
  "description": "Basic examples of the Spotify authorization flows through OAuth 2",
  "version": "0.0.2",
  "dependencies": {
    "cookie-parser": "1.3.2",
    "cors": "^2.8.4",
    "express": "^4.16.4",
    "path": "^0.12.7",
    "pg": "^7.18.1",
    "pg-hstore": "^2.3.3",
    "querystring": "~0.2.0",
    "request": "~2.83.0",
    "sequelize": "^5.21.4"
  },
  "scripts": {
    "start": "node ./authorization_code/app.js",
    "server": "nodemon ./authorization_code/app.js",
    "client": "npm start --prefix ../client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "npm install client && npm run build client"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0"
  }
}
